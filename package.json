{
  "name": "it-is-the-egg",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Daniel Harvey <danieljamesharvey@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "psc-package": "3.0.1",
    "pscid": "2.4.0",
    "pulp": "12.3.0",
    "purescript": "^0.13.0",
    "xhr2": "^0.1.3"
  },
  "scripts": {
    "postinstall": "spago install",
    "build": "spago bundle-app --to ./public/egg.js",
    "watch": "spago bundle-app -w --to ./public/egg.js",
    "serve": "serve ./public",
    "test": "spago test",
    "test:watch": "spago test -w",
    "pscid": "pscid src/Main.purs",
    "pscid:test": "pscid test/Main.purs",
    "parcel:build": "yarn test && yarn build && parcel build public/index.html"
  },
  "dependencies": {
    "parcel": "^1.12.3",
    "spago": "^0.10.0"
  }
